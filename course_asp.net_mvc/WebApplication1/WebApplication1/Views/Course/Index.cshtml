 @model CourseListAndDeptList
@{
    ViewData["Title"] = "Index";
    ITIContext _con = new ITIContext();
}

<h1>Courses</h1>
<table class=" table table-active table-bordered table-hover">
   <tr>
       <th>Name</th>
       <th>Details</th>
    

   </tr>
   @foreach(var item in Model.CourseList){

       <tr>
           <td>@item.Name</td>
          
           
            <td><a asp-action="Details" asp-controller="Course" asp-route-Id="@item.Id">Details</a></td>

       </tr>
   }

</table>

<a asp-action="AddNew" asp-controller="Course" class="btn btn-link btn-success " style="color:black; text-decoration:none;">Add_New</a>





