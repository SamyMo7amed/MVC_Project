@model List<Department>
@{
    ViewData["Title"] = "  Department Index";
}

<h1>Index</h1>
<table class="table table-hover table-active table-bordered">
    <tr>
        <th>number</th>
        <th>Name</th>

    </tr>
    @foreach(var item in Model){
        <tr>
            <td>@item.Id</td>
            <td>@item.Name</td>
        </tr>
    }
</table>

